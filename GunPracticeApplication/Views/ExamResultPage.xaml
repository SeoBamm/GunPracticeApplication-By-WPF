<Window x:Class="GunPracticeApplication.Views.ExamResultPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Exam Result Page">
    <Grid>
        <TextBlock Text="Exam Results" FontSize="24" FontWeight="Bold" Margin="10"/>

        <TextBlock Text="{Binding ExamScoreMessage}" FontSize="18" Margin="10"/>

        <Button Content="Back to Main Page" Command="{Binding BackToMainPageCommand}" Margin="10"/>
    </Grid>
</Window>